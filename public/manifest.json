{
  "manifest_version": 3,
  "name": "Marker",
  "version": "1.0",
  "description": "A chrome extension to manage your bookmarks",
  "permissions": ["bookmarks", "tabs", "storage", "unlimitedStorage"],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "icons": {
    "48": "logo.png",
    "96": "logo2x.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "48": "logo.png",
      "96": "logo2x.png"
    }
  }
}
